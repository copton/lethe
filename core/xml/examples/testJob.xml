					<port>Input</port>
				</node>
			</edge>
			<edge>
				<name>kanal2decoder</name>
				<node>
					<name>Kanal</name>
					<port>Output</port>
				</node>
				<node>
					<name>Dekodierer</name>
					<port>Input</port>
				</node>
			</edge>
			<edge>
				<name>dekodierer2senke</name>
				<node>
					<name>Dekodierer</name>
					<port>Output</port>
				</node>
				<node>
					<name>Senke</name>
					<port>Result</port>
				</node>
			</edge>
		</edges>
		<phases>
			<phase nr="1">
