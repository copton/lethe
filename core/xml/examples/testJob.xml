<simmit:job xmlns:simmit="https://proj.5nord.org/simmit" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="https://proj.5nord.org/simmit testJob.xsd"> 
	<name>Testmodul</name>
	<author>phia</author>
	<version>0.1</version>
	<date>2006-01-14</date>
	<description>Einfacher Graph</description>
	<graph>
		<nodes>
			<node>
				<name>Quelle</name>
				<location>
					<module>Source</module>
					<instance>Source</instance>
				</location>
			</node>
			<node>
				<name>Kodierer</name>
				<location>
					<module>Encoder</module>
					<instance>Encoder</instance>
				</location>
			</node>
			<node>
				<name>Kanal</name>
