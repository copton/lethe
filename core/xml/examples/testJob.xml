					<node>
						<name>Kanal</name>
						<port>
							<name>Input</name>
						</port>
						<port>
							<name>Output</name>
						</port>
					</node>
					<node>
						<name>Dekodierer</name>
						<port>
							<name>Input</name>
						</port>
						<port>
							<name>Output</name>
						</port>
					</node>
					<node>
						<name>Senke</name>
